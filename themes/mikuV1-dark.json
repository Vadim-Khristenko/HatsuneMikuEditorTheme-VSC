  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4a5280"
      }
    },
    {
      "name": "Comment Doc",
      "scope": ["comment.block.documentation"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5a6490"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#ccd6f6"
      }
    },
    {
      "name": "Variable - Built-in",
      "scope": ["variable.language", "variable.language.this"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff8fa3"
      }
    },
    {
      "name": "Variable - Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#e8c0a0"
      }
    },
    {
      "name": "Variable - Member / Property",
      "scope": ["variable.other.member", "variable.other.property"],
      "settings": {
        "foreground": "#a8c0ff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff6e8a"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff6e8a"
      }
    },
    {
      "name": "Keyword - Control",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#ff6e8a"
      }
    },
    {
      "name": "Keyword - Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#86a8ff"
      }
    },
    {
      "name": "Keyword - Return",
      "scope": ["keyword.control.return", "keyword.control.flow"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff6e8a"
      }
    },
    {
      "name": "Operator, Punctuation",
      "scope": [
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#7079a8"
      }
    },
    {
      "name": "Punctuation - Bracket",
      "scope": [
        "punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.section.block",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#7079a8"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml"],
      "settings": {
        "foreground": "#ff6e8a"
      }
    },
    {
      "name": "Tag - Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#d4a0ff"
      }
    },
    {
      "name": "HTML Tag Attribute",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffd580"
      }
    },
    {
      "name": "Function, Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#39c5bb"
      }
    },
    {
      "name": "Function - Built-in",
      "scope": ["support.function.builtin"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5df5e8"
      }
    },
    {
      "name": "Function - Decorator",
      "scope": [
        "entity.name.function.decorator",
        "punctuation.decorator",
        "meta.decorator"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffd580"
      }
    },
    {
      "name": "Constructor",
      "scope": ["entity.name.type.class", "variable.function.constructor"],
      "settings": {
        "foreground": "#39c5bb"
      }
    },
    {
      "name": "Number, Constant",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#d4a0ff"
      }
    },
    {
      "name": "String",
      "scope": ["string", "markup.inserted.git_gutter"],
      "settings": {
        "foreground": "#ffd580"
      }
    },
    {
      "name": "String - Escape",
      "scope": ["constant.character.escape", "string.regexp"],
      "settings": {
        "foreground": "#ff8fa3"
      }
    },
    {
      "name": "String - Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#00e5d6"
      }
    },
    {
      "name": "String - Symbol",
      "scope": ["constant.other.symbol", "constant.other.key"],
      "settings": {
        "foreground": "#39c5bb"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.class",
        "support.other.namespace",
        "markup.changed.git_gutter"
      ],
      "settings": {
        "foreground": "#ffd580"
      }
    },
    {
      "name": "Type",
      "scope": ["support.type", "entity.name.type", "storage.type.primitive"],
      "settings": {
        "foreground": "#86a8ff"
      }
    },
    {
      "name": "Type - Built-in",
      "scope": ["support.type.builtin", "storage.type.built-in"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#86a8ff"
      }
    },
    {
      "name": "Inherited Class",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#5df5e8"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.object.property",
        "meta.object-literal.key",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#a8c0ff"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name"
      ],
      "settings": {
        "foreground": "#a8c0ff"
      }
    },
    {
      "name": "CSS Class",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#ffd580"
      }
    },
    {
      "name": "CSS ID",
      "scope": ["entity.other.attribute-name.id"],
      "settings": {
        "foreground": "#39c5bb"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#d4a0ff"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#ffd580"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#39c5bb"
      }
    },
    {
      "name": "JSON Key - Level 3",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#86a8ff"
      }
    },
    {
      "name": "Markup - Heading",
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#39c5bb"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a8c0ff"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#d4a0ff"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#ffd580"
      }
    },
    {
      "name": "Markup - Link",
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "foreground": "#39c5bb"
      }
    },
    {
      "name": "Markup - Raw",
      "scope": ["markup.inline.raw", "markup.raw.block"],
      "settings": {
        "foreground": "#d4a0ff"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7079a8"
      }
    },
    {
      "name": "Markup - List Marker",
      "scope": ["punctuation.definition.list_item.markdown"],
      "settings": {
        "foreground": "#39c5bb"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    }
  ]
}
