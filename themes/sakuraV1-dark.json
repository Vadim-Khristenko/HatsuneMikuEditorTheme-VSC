  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6a4860"
      }
    },
    {
      "name": "Comment Doc",
      "scope": ["comment.block.documentation"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7a5870"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#f0d6e8"
      }
    },
    {
      "name": "Variable - Built-in",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff99bb"
      }
    },
    {
      "name": "Variable - Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#e8c8b0"
      }
    },
    {
      "name": "Variable - Member",
      "scope": ["variable.other.member", "variable.other.property"],
      "settings": {
        "foreground": "#d4b0cc"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff5577"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff7799"
      }
    },
    {
      "name": "Keyword - Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#e87ea1"
      }
    },
    {
      "name": "Keyword - Return",
      "scope": ["keyword.control.return", "keyword.control.flow"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff7799"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#9a7090"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ff7799"
      }
    },
    {
      "name": "Tag - Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ffaacc"
      }
    },
    {
      "name": "HTML Attribute",
      "scope": ["text.html.basic entity.other.attribute-name"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffcc88"
      }
    },
    {
      "name": "Function, Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#39c5bb"
      }
    },
    {
      "name": "Function - Built-in",
      "scope": ["support.function.builtin"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5df5e8"
      }
    },
    {
      "name": "Function - Decorator",
      "scope": ["entity.name.function.decorator", "meta.decorator"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffcc88"
      }
    },
    {
      "name": "Number, Constant",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#ffaacc"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#ffcc88"
      }
    },
    {
      "name": "String - Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#ff99bb"
      }
    },
    {
      "name": "String - Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#88ddcc"
      }
    },
    {
      "name": "String - Symbol",
      "scope": ["constant.other.symbol", "constant.other.key"],
      "settings": {
        "foreground": "#e87ea1"
      }
    },
    {
      "name": "Class, Type",
      "scope": ["entity.name", "support.class"],
      "settings": {
        "foreground": "#ffcc88"
      }
    },
    {
      "name": "Type",
      "scope": ["support.type", "entity.name.type", "storage.type.primitive"],
      "settings": {
        "foreground": "#99aaff"
      }
    },
    {
      "name": "Type - Built-in",
      "scope": ["support.type.builtin"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#99aaff"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.object.property",
        "meta.object-literal.key",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#d4b0cc"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name"
      ],
      "settings": {
        "foreground": "#d4b0cc"
      }
    },
    {
      "name": "CSS Class",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#ffcc88"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#ffaacc"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#ffcc88"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#e87ea1"
      }
    },
    {
      "name": "Markup - Heading",
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e87ea1"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#99aaff"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffaacc"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#ffcc88"
      }
    },
    {
      "name": "Markup - Link",
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "foreground": "#e87ea1"
      }
    },
    {
      "name": "Markup - Raw",
      "scope": ["markup.inline.raw", "markup.raw.block"],
      "settings": {
        "foreground": "#ffaacc"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9a7090"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    }
  ]
}
