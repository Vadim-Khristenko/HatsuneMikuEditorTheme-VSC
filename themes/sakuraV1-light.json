  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c088a0"
      }
    },
    {
      "name": "Comment Doc",
      "scope": ["comment.block.documentation"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a87090"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#3a1428"
      }
    },
    {
      "name": "Variable - Built-in",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ee3366"
      }
    },
    {
      "name": "Variable - Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#6a3020"
      }
    },
    {
      "name": "Variable - Member",
      "scope": ["variable.other.member", "variable.other.property"],
      "settings": {
        "foreground": "#7a2244"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#cc2244"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#cc2244"
      }
    },
    {
      "name": "Keyword - Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#2244aa"
      }
    },
    {
      "name": "Keyword - Return",
      "scope": ["keyword.control.return", "keyword.control.flow"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#cc2244"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#a87090"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#cc2244"
      }
    },
    {
      "name": "Tag - Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#cc4477"
      }
    },
    {
      "name": "HTML Attribute",
      "scope": ["text.html.basic entity.other.attribute-name"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#996600"
      }
    },
    {
      "name": "Function, Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#227766"
      }
    },
    {
      "name": "Function - Built-in",
      "scope": ["support.function.builtin"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#339988"
      }
    },
    {
      "name": "Function - Decorator",
      "scope": ["entity.name.function.decorator", "meta.decorator"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#996600"
      }
    },
    {
      "name": "Number, Constant",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#aa2255"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#996600"
      }
    },
    {
      "name": "String - Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#cc2244"
      }
    },
    {
      "name": "String - Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#227766"
      }
    },
    {
      "name": "String - Symbol",
      "scope": ["constant.other.symbol", "constant.other.key"],
      "settings": {
        "foreground": "#cc4477"
      }
    },
    {
      "name": "Class, Type",
      "scope": ["entity.name", "support.class"],
      "settings": {
        "foreground": "#996600"
      }
    },
    {
      "name": "Type",
      "scope": ["support.type", "entity.name.type", "storage.type.primitive"],
      "settings": {
        "foreground": "#2244aa"
      }
    },
    {
      "name": "Type - Built-in",
      "scope": ["support.type.builtin"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2244aa"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.object.property",
        "meta.object-literal.key",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#7a2244"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name"
      ],
      "settings": {
        "foreground": "#7a2244"
      }
    },
    {
      "name": "CSS Class",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#996600"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#cc4477"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#996600"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#227766"
      }
    },
    {
      "name": "Markup - Heading",
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#cc4477"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2244aa"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#aa2255"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#996600"
      }
    },
    {
      "name": "Markup - Link",
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "foreground": "#cc4477"
      }
    },
    {
      "name": "Markup - Raw",
      "scope": ["markup.inline.raw", "markup.raw.block"],
      "settings": {
        "foreground": "#aa2255"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a87090"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    }
  ]
}

