  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#88b0ae"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#1a3a3a"
      }
    },
    {
      "name": "Variable - Built-in",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#cc4466"
      }
    },
    {
      "name": "Variable - Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#5a3a1a"
      }
    },
    {
      "name": "Variable - Member",
      "scope": ["variable.other.member", "variable.other.property"],
      "settings": {
        "foreground": "#1a6688"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#cc2244"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#cc2244"
      }
    },
    {
      "name": "Keyword - Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#2255aa"
      }
    },
    {
      "name": "Keyword - Return",
      "scope": ["keyword.control.return", "keyword.control.flow"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#cc2244"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#6a9896"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#cc2244"
      }
    },
    {
      "name": "Tag - Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#8833bb"
      }
    },
    {
      "name": "HTML Attribute",
      "scope": ["text.html.basic entity.other.attribute-name"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b07800"
      }
    },
    {
      "name": "Function, Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#007a72"
      }
    },
    {
      "name": "Function - Built-in",
      "scope": ["support.function.builtin"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#339988"
      }
    },
    {
      "name": "Function - Decorator",
      "scope": ["entity.name.function.decorator", "meta.decorator"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b07800"
      }
    },
    {
      "name": "Number, Constant",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#8833bb"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#b07800"
      }
    },
    {
      "name": "String - Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#cc2244"
      }
    },
    {
      "name": "String - Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#007a72"
      }
    },
    {
      "name": "String - Symbol",
      "scope": ["constant.other.symbol", "constant.other.key"],
      "settings": {
        "foreground": "#007a72"
      }
    },
    {
      "name": "Class, Type",
      "scope": ["entity.name", "support.class"],
      "settings": {
        "foreground": "#b07800"
      }
    },
    {
      "name": "Type",
      "scope": ["support.type", "entity.name.type", "storage.type.primitive"],
      "settings": {
        "foreground": "#2255aa"
      }
    },
    {
      "name": "Type - Built-in",
      "scope": ["support.type.builtin"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2255aa"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.object.property",
        "meta.object-literal.key",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#1a6688"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name"
      ],
      "settings": {
        "foreground": "#1a6688"
      }
    },
    {
      "name": "CSS Class",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#b07800"
      }
    },
    {
      "name": "Markup - Heading",
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#007a72"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2255aa"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#8833bb"
      }
    },
    {
      "name": "Markup - Link",
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "foreground": "#007a72"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    }
  ]
}
